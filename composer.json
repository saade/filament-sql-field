{
    "name": "saade/filament-sql-field",
    "description": "A SQL field with tables and columns autocompletion",
    "keywords": [
        "saade",
        "laravel",
        "filament-sql-field"
    ],
    "homepage": "https://github.com/saade/filament-sql-field",
    "support": {
        "issues": "https://github.com/saade/filament-sql-field/issues",
        "source": "https://github.com/saade/filament-sql-field"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Saade",
            "email": "saade@outlook.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Saade\\FilamentSqlField\\": "src/",
            "Saade\\FilamentSqlField\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saade\\FilamentSqlField\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saade\\FilamentSqlField\\FilamentSqlFieldServiceProvider"
            ],
            "aliases": {
                "FilamentSqlField": "Saade\\FilamentSqlField\\Facades\\FilamentSqlField"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}